{
  "name": "jtrw/dao-dbal-adapter",
  "type": "library",
  "description": "Data Access Object for fork with DataBases",
  "keywords": [
    "nilBora",
    "DAO",
    "Data Access Object",
    "DB",
    "Data Base"
  ],
  "homepage": "https://github.com/jtrw/dao-dbal-adapter",
  "license": "MIT",
  "authors": [
    {
      "name": "Nil",
      "email": "brdnlsrg@gmail.com",
      "homepage": "https://github.com/jtrw",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "jtrw/dao": "*",
    "doctrine/dbal": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Jtrw\\DAO\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jtrw\\DAO\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "config": {
    "sort-packages": true
  }
}
